<coherence xmlns="http://schemas.tangosol.com/coherence">
  <logging-config>
    <destination>stdout</destination>
    <severity-level>5</severity-level>
    <message-format>{date} {product} {version} &lt;{level}&gt; (thread={thread}, member={member}): {text}</message-format>
    <character-limit>8192</character-limit>
  </logging-config>

  <configurable-cache-factory-config>
    <class-name>dummy</class-name>
    <init-params>
      <init-param>
        <param-name>long</param-name>
        <param-value>101</param-value>
      </init-param>
      <init-param>
        <param-name>date</param-name>
        <param-value>2/16/1992 12:15:12</param-value>
      </init-param>
      <init-param>
        <param-name>float</param-name>
        <param-value>0.00000001</param-value>
      </init-param>
    </init-params>

  </configurable-cache-factory-config>
</coherence>
